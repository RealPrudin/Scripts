local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local LBB = library:CreateWindow("Lucky Blocks Battlegrounds")

local SpawnBlocks = LBB:CreateFolder("Spawn Lucky Blocks")

SpawnBlocks:Button("Lucky Block",function()
    game:GetService("ReplicatedStorage").SpawnLuckyBlock:FireServer()
end)

SpawnBlocks:Button("Super Lucky Block",function()
    game:GetService("ReplicatedStorage").SpawnSuperBlock:FireServer()
end)

SpawnBlocks:Button("Diamond Lucky Block",function()
    game:GetService("ReplicatedStorage").SpawnDiamondBlock:FireServer()
end)

SpawnBlocks:Button("Rainbow Lucky Block",function()
    game:GetService("ReplicatedStorage").SpawnRainbowBlock:FireServer()
end)

SpawnBlocks:Button("Galaxy Lucky Block",function()
    game:GetService("ReplicatedStorage").SpawnGalaxyBlock:FireServer()
end)
