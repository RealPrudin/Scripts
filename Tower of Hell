local library = loadstring(game:HttpGet(('https://pastebin.com/raw/FsJak6AT')))()

local TOF = library:CreateWindow("Tower of Hell")
local Teleport = TOF:CreateFolder("Teleport")
local Settings = TOF:CreateFolder("Settings")
local Credits = TOF:CreateFolder("Credits")
local player = game.Players.LocalPlayer
local target = game.Workspace.tower.finishes.Finish

Teleport:Button("Teleport to end",function()
    player.Character.HumanoidRootPart.CFrame = target.CFrame * CFrame.new(0, 3, 0)
end)

Settings:DestroyGUI()
Settings:GuiSettings()

Credits:Label("Made by Prudin#6667",Color3.fromRGB(38,38,38),Color3.fromRGB(0,216,111))
